{
    "name": "qwat-drift",
    "description": "Drift Chat Backend and Frontend integration for Contao, a Open Source CMS",
    "keywords": ["contao", "Drift", "live chat", "chat", "support"],
    "type": "contao-module",
    "license": "LGPLv3",
    "authors": [
        {
            "name": "Wolfgang Schwaiger",
            "homepage": "http://qualitywork.at"
        }
    ],
    "require": {
        "php": ">=5.3.2",
        "contao/core": ">=3.5",
        "contao-community-alliance/composer-plugin": "2.*"
    },
    "replace": {
        "contao-legacy/qwat-drift": "self.version"
    },
    "extra": {
        "contao": {
            "sources": {
                "": "system/modules/qwat-drift"
            }
        }
    }
}
